_shtab_guv_options_=(
  "(- :)"{-h,--help}"[show this help message and exit]"
)

_shtab_guv_commands_() {
  local _commands=(
    "add_instructors:Ajoute les intervenants dans la liste csv des créneaux"
    "attendance_sheet:Fichiers pdf de feuilles de présence sans les noms des étudiants."
    "attendance_sheet_room:Fichier zip de feuilles de présence nominatives par taille des salles"
    "cal_inst:Calendrier PDF de la semaine par intervenant"
    "cal_uv:Calendrier PDF de la semaine par UV"
    "createsemester:Crée un dossier de semestre"
    "createuv:Crée des dossiers d\'UV"
    "csv_all_courses:Fichier csv de tous les créneaux du semestre"
    "csv_create_groups:Création de groupes d\'étudiants prêt à charger sous Moodle"
    "csv_exam_groups:Fichier csv des demi-groupe de TP pour le passage des examens de TP"
    "csv_for_upload:Fichier csv de notes prêtes à être chargées sur l\'ENT."
    "csv_groups:Fichiers csv des groupes de Cours\/TD\/TP\/singleton pour Moodle"
    "csv_groups_groupings:Fichier csv de groupes et groupements à charger sur Moodle pour les créer"
    "csv_inscrits:Construit un fichier CSV à partir des données brutes de la promo fournies par l\'UTC"
    "csv_moodle_groups:Fichier csv de sous-groupes (binômes ou trinômes) aléatoires"
    "doit:Permet d\'avoir accès aux commandes doit sous-jacentes"
    "fetch_group_id:Crée un fichier de correspondance entre le nom des groupes Moodle et leur id"
    "html_inst:Génère la description des intervenants pour Moodle"
    "html_table:Table HTML des Cours\/TD\/TP à charger sur Moodle"
    "ical_inst:Fichier iCal de tous les créneaux par intervenant"
    "json_group:Fichier json des restrictions d\'accès aux ressources sur Moodle par addresse email "
    "json_restriction:Ficher json de restrictions d\'accès aux ressources sur Moodle basées sur le début\/fin des séances"
    "pdf_attendance_full:Fichier zip de feuilles de présence nominatives par groupe et par semestre"
    "pdf_attendance_list:Fichier pdf de feuilles de présence nominatives par groupes"
    "pdf_trombinoscope:Fichier PDF des trombinoscopes par groupes et\/ou sous-groupes"
    "tabcompletion:Écrit un fichier d\'autocomplétion pour zsh"
    "utc_uv_list_to_csv:Crée un fichier CSV à partir de la liste d\'UV au format PDF"
    "xls_affectation:Fichier Excel des créneaux de toutes les UV configurées."
    "xls_assignment_grade:Création d\'un fichier Excel pour remplissage des notes par les intervenants"
    "xls_grade_sheet:Génère un fichier Excel pour faciliter la correction des examens\/projets\/jury"
    "xls_inst_details:Fichier Excel des intervenants par UV avec détails"
    "xls_instructors:Fichier de détails global des intervenants toutes UV confondues"
    "xls_merge_final_grade:Fichier Excel des notes finales attribuées"
    "xls_student_data:Construit le fichier Excel des données étudiants fournies par l\'UTC"
    "xls_student_data_merge:Ajoute toutes les autres informations étudiants"
    "xls_utp:Crée un document Excel pour calcul des heures et remplacements."
    "yaml_qcm:Génère un fichier yaml prérempli pour noter un QCM"
    "zoom_breakout_rooms:Crée un fichier csv prêt à charger sur Zoom pour faire des groupes"
  )

  _describe 'guv commands' _commands
}

